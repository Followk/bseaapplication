<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:monkey="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:clipChildren="false"
    tools:context="com.dk.basepack.bseaapplication.MainActivity">




    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />


    <com.dk.basepack.bseaapplication.weight.CustomizedBottomNavigationBar
        android:id="@+id/navigation_bar"
        android:layout_width="match_parent"
        android:clipChildren="false"
        android:layout_height="47dp"
        android:layout_alignParentBottom="true"></com.dk.basepack.bseaapplication.weight.CustomizedBottomNavigationBar>

</LinearLayout>
